<audio id="troll"></audio>
<script type="text/javascript">
  var audio = document.getElementById("troll");
	audio.src = (Math.random() % 2 == 0) ? "/images/Trololo.mp3" : "/images/rick.mp3";
	audio.volume = 0;
	document.getElementById("troll").play();
  setTimeout(function(){
    document.getElementById("troll").volume = 1;
    console.log('your audio is started just now');
  }, 3000);
</script>